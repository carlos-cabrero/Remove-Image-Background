    "# Import 'rembg' \n",
    "from rembg import remove\n",
    "from PIL import Image"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Remove image background\n",
    "input_path = 'bg_nike.jpg'\n",
    "output_path = 'bg_nike_output.png'\n",
    "\n",
    "input = Image.open(input_path)\n",
    "\n",
    "output = remove(input)\n",
    "output.save(output_path)"
   ]
